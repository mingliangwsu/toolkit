MAKEFILE      = Makefile

####### Compiler, tools and options

CC            = gcc
CXX           = g++
DEFINES       = -DHISTORICAL_MET 
CFLAGS        = -pipe -g -Wall -W -D_REENTRANT -fPIC $(DEFINES)
CXXFLAGS      = -pipe -g -std=gnu++0x -Wall -W -D_REENTRANT -fPIC $(DEFINES)
INCPATH       = -I. 
DISTNAME      = readUINetCDF1.0.0
LINK          = g++
LFLAGS        = -Wl
LIBS          = $(SUBLIBS) -lnetcdf
####### Files
SOURCES       = main.cpp \
		time_tools.cpp 
OBJECTS       = main.o \
		time_tools.o
TARGET        = readUINetCDF


first: all
####### Build rules

$(TARGET):  $(OBJECTS)  
	$(LINK) $(LFLAGS) -o $(TARGET) $(OBJECTS) $(OBJCOMP) $(LIBS)

all: Makefile $(TARGET)


####### Compile

main.o: main.cpp 
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o main.o main.cpp

time_tools.o: time_tools.cpp UInetcdf_constants.h
	$(CXX) -c $(CXXFLAGS) $(INCPATH) -o time_tools.o time_tools.cpp


